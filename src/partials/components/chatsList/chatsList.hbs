<div class="chats-list">
    <div class="chats-list__header">
        {{> ui/divider color="secondary" style="margin: 2px 10px"}}
    </div>
    <div class="chats-list__list">
        {{#each chats}}
            {{> components/chatsList/chatsListItem username=this.username time=this.time message=this.message messagesCount=this.messagesCount}}
            {{> ui/divider color="secondary" style="margin: 2px 10px"}}
        {{/each}}
    </div>
</div>
